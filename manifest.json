{
  "name": "Pixel.ee - Block Tann",
  "version": "0.1",
  "description": "Blocks Tann posts in Pixel.ee community",
  "permissions": ["http://*.pixel.ee/", "https://*.pixel.ee/", "http://pixel.ee/", "https://pixel.ee/"],
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Stop troll!"
  },
  "content_scripts": [
    {
      "matches": ["http://*.pixel.ee/*", "https://*.pixel.ee/*", "http://pixel.ee/*", "https://pixel.ee/*"],
      "js": ["stop-troll.js"]
    }
  ],
  "icons": {
    "128": "stop-troll.png" 
  }
}
